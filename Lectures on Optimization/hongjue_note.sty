%% Related Commands
%% ----------------------------------
\setlist[enumerate]{itemsep = 0.8pt}
\setlist[itemize]{itemsep = 0.8pt}
\raggedbottom
\linespread{1.2} 
\counterwithin{equation}{section}
% \setlength{\parindent}{0pt} 

\SetKwInOut{KwIn}{\textbf{Input}}
\SetKwInOut{KwOut}{\textbf{Output}}
\SetKwComment{KwComment}{/*}{*/}

\newcommand{\R}{\mathbb{R}}
\newcommand{\N}{\mathbb{N}}
\newcommand{\innerprod}[2]{\left\langle #1, #2 \right\rangle}
% \newcommand{\myvec}[1]{\boldsymbol{#1}}
\newcommand{\norminf}[1]{\norm{#1}_{\infty}}
\newcommand{\solution}{\bar{\bm{x}}}
\newcommand{\dom}{\mathrm{dom}~}
\newcommand{\epi}{\mathrm{epi}}
\newcommand{\Conv}[1]{\mathrm{Conv}\left\{#1\right\}}
\newcommand{\xB}{\bm{x}}
\newcommand{\yB}{\bm{y}}
\newcommand{\gB}{\bm{g}}
\newcommand{\pB}{\bm{p}}

\renewcommand{\grad}{\nabla}
\newcommand{\hess}{\laplacian}
\newtheorem[M]{defn}{Definition}[section]
\newtheorem[M]{thm}{Theorem}[section]
\newtheorem[S]{assum}{Assumption}[section]
\newtheorem[S]{coro}{Corollary}[section]
\newtheorem[S]{lemma}{Lemma}[section]
\newtheorem[S]{eg}{e.g.}[section]

\newenvironment{note}[1]{\textbf{{#1}:}}{}
\newenvironment{boxnote}[1]{
    \begin{tcolorbox}[
        colback = white, colframe=black, title = #1
    ]
}{\end{tcolorbox}}
\newenvironment{colorboxnote}[1]{
    \begin{tcolorbox}[
        colback = gray!10, colframe=gray!60!black, title = #1
    ]
}{\end{tcolorbox}}